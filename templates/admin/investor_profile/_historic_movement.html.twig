<div class="col-12 col-md-6 mt-4">
    <div class="card h-100">
        <div class="card-header pb-0 p-3">
            <div class="row">
                <div class="col-md-8 d-flex align-items-center">
                    <h6 class="mb-0">Historic movements</h6>
                </div>

            </div>
        </div>
        <div class="card-body p-3 overflow-auto" style="max-height: 400px;">
            {% if movements | length > 0 %}
                {% include "admin/investor_profile/_table_movement.html.twig" %}
            {% else %}
                <h6 class="text-uppercase text-body text-xs font-weight-bolder">No movement</h6>
            {% endif %}
        </div>
    </div>
</div>
<div class="col-12 col-md-6 mt-4">
    <div class="card h-100">
        <div class="card-header pb-0 p-3">
            <h6 class="mb-0">Line chart movements</h6>
        </div>
        {% if chartLine %}
        <div class="card-body">
            {{ render_chart(chartLine) }}
        </div>
        {% endif %}
    </div>
</div>