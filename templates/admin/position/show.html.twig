<!DOCTYPE html>

<title>Position</title>

{% block body %}
    <h1>Position</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ position.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ position.name }}</td>
            </tr>
            <tr>
                <th>Tp1</th>
                <td>{{ position.tp1 }}</td>
            </tr>
            <tr>
                <th>Tp2</th>
                <td>{{ position.tp2 }}</td>
            </tr>
            <tr>
                <th>Tp3</th>
                <td>{{ position.tp3 }}</td>
            </tr>
            <tr>
                <th>Tp4</th>
                <td>{{ position.tp4 }}</td>
            </tr>
            <tr>
                <th>Tp5</th>
                <td>{{ position.tp5 }}</td>
            </tr>
            <tr>
                <th>Tp6</th>
                <td>{{ position.tp6 }}</td>
            </tr>
            <tr>
                <th>Tp7</th>
                <td>{{ position.tp7 }}</td>
            </tr>
            <tr>
                <th>Tp8</th>
                <td>{{ position.tp8 }}</td>
            </tr>
            <tr>
                <th>StopLoss</th>
                <td>{{ position.stopLoss }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ position.createdAt ? position.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>SellAt</th>
                <td>{{ position.sellAt }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ position.isActive ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('position_index') }}">back to list</a>

    <a href="{{ path('position_edit', {'id': position.id}) }}">edit</a>

    {{ include('position/_delete_form.html.twig') }}
{% endblock %}
