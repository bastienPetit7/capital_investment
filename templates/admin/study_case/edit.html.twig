        <div class="row">
            {% include "shared/_messages_flash.html.twig" %}
            <div class="col-12">
                <div class="card card-frame">
                    <div class="card-header">
                        <h3><i class="fas fa-pen"></i> Edit a study case</h3>
                    </div>
                    <div class="card-body">
                        {{ form_start(form) }}

                        {{ form_widget(form) }}
